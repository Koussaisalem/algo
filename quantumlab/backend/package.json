{
  "name": "quantumlab-backend",
  "version": "1.0.0",
  "scripts": {
    "start": "uvicorn inference_server:app --host 0.0.0.0 --port 8000",
    "dev": "uvicorn inference_server:app --reload --host 0.0.0.0 --port 8000",
    "test": "pytest tests/ -v --cov=. --cov-report=html --cov-report=term",
    "test:watch": "pytest-watch",
    "lint": "black . --check && flake8 . && bandit -r . -f screen",
    "format": "black . && isort .",
    "type-check": "mypy .",
    "security": "bandit -r . -f json -o security-report.json && pip-audit",
    "docs": "python -m pydoc -b",
    "migrate": "alembic upgrade head",
    "migrate:create": "alembic revision --autogenerate -m"
  },
  "dependencies": {
    "fastapi": ">=0.104.0",
    "uvicorn[standard]": ">=0.24.0",
    "torch": ">=2.0.0",
    "schnetpack": ">=2.0.0",
    "psutil": ">=5.9.0",
    "paramiko": ">=3.3.0",
    "cryptography": ">=41.0.0",
    "sqlalchemy": ">=2.0.0",
    "asyncpg": ">=0.29.0",
    "pg8000": ">=1.30.0",
    "pydantic": ">=2.0.0",
    "python-multipart": ">=0.0.6",
    "websockets": ">=11.0.0"
  },
  "devDependencies": {
    "pytest": ">=7.4.0",
    "pytest-cov": ">=4.1.0",
    "pytest-asyncio": ">=0.21.0",
    "pytest-watch": ">=4.2.0",
    "black": ">=23.0.0",
    "flake8": ">=6.0.0",
    "isort": ">=5.12.0",
    "mypy": ">=1.5.0",
    "bandit": ">=1.7.5",
    "pip-audit": ">=2.6.0",
    "locust": ">=2.15.0",
    "faker": ">=19.0.0"
  }
}
